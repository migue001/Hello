<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.tensorflow.demo.MainActivity"
    android:background="@color/colorPrimary"
    >

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:text="Ingresa tu Nombre"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold" />

    <EditText

        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:background="@drawable/boton1"
        android:drawableLeft="@drawable/icon_mdpi"
        android:ems="10"
        android:gravity="center"
        android:hint="Usuario"
        android:inputType="textPersonName"

        android:textColor="@color/white"
        android:textColorHint="@color/white" />

    <TextView
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignRight="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:background="@drawable/boton1"
        android:ems="10"
        android:textSize="16dp"
        android:gravity="center"
        android:hint="dd/MM/yyyy"
        android:textColorHint="@color/white"
        android:textColor="@color/white"

        />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="16dp"
        android:text="Fecha de Nacimiento:"
        android:textColor="@color/white"
        android:textSize="20dp"
        android:textStyle="bold"
        />

    <Button
        android:id="@+id/button"
        android:layout_width="100dp"
        android:layout_height="70dp"

        android:layout_below="@+id/editText5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp"
        android:background="@drawable/boton2"
        android:onClick="inicio"
        android:text="Aceptar"
        android:textColor="@color/white"
        android:textSize="15dp"
        android:textStyle="bold" />

    <at.markushi.ui.CircleButton
        android:id="@+id/floatt2"
        android:layout_width="60dip"
        android:layout_height="60dip"
        android:layout_below="@+id/editText2"
        android:layout_centerHorizontal="true"
        android:src="@drawable/calendarios"
app:cb_color="@color/iconis2"
         />
    <!--android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_below="@+id/editText2"
       android:layout_centerHorizontal="true"
       android:layout_marginTop="14dp"
       app:backgroundTint="@color/iconis2"
       android:src="@drawable/calendarios"-->
    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/floatt2"
        android:layout_centerHorizontal="true"
        android:text="Agrega algun numero telefonico de Confianza \npor si necesitas ayuda: "
        android:textColor="@color/white"
        android:textStyle="bold"
        android:gravity="center"
        />

<EditText
    android:id="@+id/editText3"
    android:layout_width="wrap_content"
    android:layout_height="40dp"
    android:layout_below="@+id/textView5"
    android:layout_centerHorizontal="true"
    android:background="@drawable/boton1"
    android:ems="10"
    android:gravity="center"
    android:hint="Numero"
    android:inputType="phone"
    android:textColor="@color/white"
    android:textColorHint="@color/white" />

<TextView
    android:id="@+id/textView8"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/editText3"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="22dp"
    android:text="Agrega una Frase para Poder Llamar en el momento a la Persona"
    android:textColor="@color/white"
    android:textStyle="bold"
    android:gravity="center"
    />

<EditText
    android:id="@+id/editText5"
    android:layout_width="wrap_content"
    android:layout_height="40dp"
    android:layout_below="@+id/textView8"
    android:layout_centerHorizontal="true"
    android:background="@drawable/boton1"
    android:ems="10"
    android:gravity="center"
    android:hint="Frase"
    android:inputType="textPersonName"
    android:textColor="@color/white"
    android:textColorHint="@color/white" />


</RelativeLayout>
